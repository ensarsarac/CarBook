
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("~/Views/AdminLayout/_HeadPartial.cshtml")
<body>
    <div id="layout-wrapper">
        @await Html.PartialAsync("~/Views/AdminLayout/_NavbarPartial.cshtml")
        @await Html.PartialAsync("~/Views/AdminLayout/_LeftSideBarPartial.cshtml")
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    @RenderBody()     
                </div> 
            </div>
            @await Html.PartialAsync("~/Views/AdminLayout/_FooterPartial.cshtml")
        </div>
    </div>
    <div class="menu-overlay"></div>
    @await Html.PartialAsync("~/Views/AdminLayout/_ScriptPartial.cshtml")
</body>
</html>
